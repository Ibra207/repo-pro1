<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    
    <remote name="origin"
         fetch="https://github.com/Ibra207"/>

    <remote  name="remote-projet1"
            fetch="https://github.com/Ibra207">
            
    <remote  name="remote-projet2"
            fetch="https://github.com/Ibra207"/>
   
  
  <default
      remote="origin"
        sync-c="true"
        sync-j="4" />
 
  <!-- extend project in local manifest -->
   <project
        name="repo-projet1"
        path="projet1"
        revision="feature"
        remote="remote-projet1"/> 
   <project
        name="repo-projet2"
        path="projet2"
        revision="feature"
        remote="remote-projet2"/> 
 
</manifest>
